{
  "name": "thapp/image",
  "description": "Image processor",
  "keywords": ["image", "GD", "imagemagick", "Imagick"],
  "require": {
    "php": ">=5.6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.2@dev"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "iwyg",
      "email": "mail@thomas-appel.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Thapp\\Image\\":"src/"
    },
    "files": [
      "src/Polyfill/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Thapp\\Image\\Tests\\":"tests/"
    }
  },
  "suggest": {
    "php-imagick": "Install the imagick extension for best experience."
  },
  "minimum-stability": "dev"
}
